{
  "version": 2,
  "builds": [
    { "src": "client/vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "client/dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://your-render-service.onrender.com/api/$1" },
    { "src": "/(.*)", "dest": "/client/dist/index.html" }
  ]
}
